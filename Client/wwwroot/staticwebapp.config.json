{
  "routes": [
    {
      "route": "/admin/*",
      "allowedRoles": [ "admin" ]
    },
    {
      "route": "/api/pagetext",
      "methods": [ "POST" ],
      "allowedRoles": [ "admin" ]
    }
  ],
  "navigationFallback": { "rewrite": "index.html" },
  "responseOverrides": {
    "401": { "rewrite": "/login" },
    "403": { "rewrite": "/login" }
  }
}
